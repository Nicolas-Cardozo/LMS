create database lms;
use lms;

CREATE TABLE Disciplina(
nome varchar(30) NOT NULL
plano_de_ensino varchar(100)
carga_horaria int NOT NULL
Competencias varchar(40) NOT NULL
habilidades varchar(30)NOT NULL
ementa varchar(120)NOT NULL
conteudo_programatico varchar(120)NOT NULL
blibiografia_basica varchar(80)NOT NULL
bibliografia_complementar varchar(120)NOT NULL
percentual_pratico int NOT NULL
percentual_teorico int NOT NULL
Constraint pkDisciplina PRIMARY KEY(nome)
Constraint FKDisciplina FOREIGN KEY(ID_Coordenador)
References Coordenador (ID_coordenador)		 
);

CREATE TABLE administra (
data_ad date NOT NULL
status_ad varchar(30)NOT NULL
);

